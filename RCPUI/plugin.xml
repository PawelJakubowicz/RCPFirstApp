<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.eclipse.e4.ui.workbench.swt.E4Application"
            name="rcpui">
         <property
               name="applicationCSS"
               value="platform:/plugin/RCPUI/css/default.css">
         </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="rcpui.ViewPart"
            id="RCPUI.view"
            name="MyView"
            restorable="true">
      </view>
   </extension>

</plugin>
